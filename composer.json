{
    "name": "bonecreative/day-villas",
    "description": "Laravel package boilerplate.",
    "keywords": [
        "laravel",
        "package"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarek Adam",
            "email": "tarek@fuqu.io"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "BoneCreative\\DayVillas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoneCreative\\Test\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "graham-campbell/testbench": "^5.0@dev",
        "phpunit/phpunit": "^7.1@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BoneCreative\\DayVillas\\ServiceProvider"
            ]
        }
    }
}
