{
    "name": "tarek-adam/day-villas",
    "description": "Laravel package boilerplate.",
    "keywords": [
        "laravel",
        "package"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Tarek Adam",
            "email": "tarek@fuqu.io"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TarekAdam\\DayVillas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TarekAdam\\Test\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url":"https://github.com/tarekadam/page-stream"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "tarek-adam/page-stream": "dev-master",
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0@dev",
        "phpunit/phpunit": "^7.1@dev",
        "mockery/mockery": "^1.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TarekAdam\\DayVillas\\ServiceProvider"
            ]
        }
    }
}
